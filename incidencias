  <script>

function confirmLogout() {
    // Mostrar un mensaje de confirmación
    if (confirm("¿Estás seguro de que deseas cerrar sesión?")) {
        // Si el usuario acepta, lanza el evento de cierre de sesión
        window.location.href = "index.html";
    }
}



$(document).ready(function() {
  
  
 document.getElementById("txtUsuario").value = localStorage.getItem("nombre") +" "+localStorage.getItem("apellidop") +" "+ localStorage.getItem("apellidom");

  
} );

  </script>




